<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mercury.gabadam.quickreports">

    <uses-permission android:name="android.permission.INTERNET" />

    <permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.mercury.gabadam.quickreports.MainActivity">
            <intent-filter>
            <action android:name="com.example.user.projecttest.MainActivity" />
            <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.mercury.gabadam.quickreports.Detail" />
        <activity android:name="com.mercury.gabadam.quickreports.ReportMainActivity" />
        <activity android:name="com.mercury.gabadam.quickreports.ReportDetail" />
        <activity android:name="com.mercury.gabadam.quickreports.LoginActivity"></activity>
    </application>

</manifest>
